w start
r Changeset tmp1 started.
w create host h1
w create host h2
w all host
w vram 4
<dump>
host h1
    no hardware
    no note_host
    no service
    no template_host
    virtual_hardware h1
end
host h2
    no hardware
    no note_host
    no service
    no template_host
    virtual_hardware h2
end
virtual_hardware h1
    host h1
    no vcpu_num
    vram 4
end
virtual_hardware h2
    host h2
    no vcpu_num
    vram 4
end
</dump>
w abort
r Changeset tmp1 aborted.
w start
r Changeset tmp2 started.
w host h3
w end
w host where (note_host != "pwn")
w cpu_ht true
