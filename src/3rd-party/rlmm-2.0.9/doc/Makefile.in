	if test -f $@ ; then : ; else rm -f $< ; $(MAKE) $<; fi

html/index.html: doxyconfig header.html footer.html style.css $(EXTRA_DIST)
	$(DOXYGEN) $<  
	if test -f logo.png ; then cp logo.png html ; fi
	if test -f favicon.png ; then cp favicon.png html ; fi

