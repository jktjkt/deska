    formfactor rackmount
    accepts_inside [sgi-twin]
    width 1
    height 1
    depth 1
    internal_width 2
    internal_depth 1
    internal_height 1
    note "A chassis for housing of two SGI twin nodes"
end

create modelbox sgi-twin
